    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "openapi-typescript": "^7.10.1",
    "source-map-loader": "^4.0.1",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "webpack": "^5.76.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "dependencies": {
    "@blockly/field-slider": "^7.0.12",
    "@ljharb/tsconfig": "^0.3.2",
