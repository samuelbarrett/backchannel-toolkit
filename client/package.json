    "@mui/material": "^6.5.0",
    "@types/express": "^4.17.25",
    "debug": "^4.4.3",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "openapi-fetch": "^0.15.0",
    "react": "^19.2.3",
